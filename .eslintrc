{
    "parser": "@babel/eslint-parser",
    "extends":["airbnb-base"],
    "plugins": ["prettier"],
    "rules":{
        "no-console":"off",
        "prettier/prettier": ["error", { "singleQuote":true }],
        "comma-dangle":0,
        "object-curly-newline": ["error", {
            "ObjectPattern": { "multiline": true }
        }]
    },
    "env": {
        "es6":true,
        "node":true,
        "browser": true
    },
    "settings": {
        "import/resolver" : { "babel-module" : {}}
    },
    "globals": {
        "__rootdir": "readonly",
        "M": "writable"
    }

}