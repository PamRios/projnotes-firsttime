{
    "parser": "@babel/eslint-parser",
    "extends":["airbnb-base"],
    "plugins": ["prettier"],
    "rules":{
        "no-console":"off",
        "prettier/prettier": ["error", { "singleQuote":true }]
    },
    "env": {
        "es6":true,
        "node":true,
        "browser": true
    },
    "settings": {
        "import/resolver":{"babel-module":{}}
    }

}